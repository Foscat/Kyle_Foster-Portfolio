{
  "name": "react-portfolio",
  "private": true,
  "version": "1.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix-safe": "eslint src --fix",
    "lint:report": "eslint src --format html --output-file docs/lint-report.html",
    "preview": "vite preview",
    "test": "vitest run",
    "codemod:fix-jsdoc": "jscodeshift -t codemods/fix-jsdoc-import-types.js src",
    "docs:clean": "rimraf docs",
    "docs:components": "jsdoc2md \"src/components/**/*.jsx\" \"src/components/**/*.js\" > docs/components.md",
    "docs:navigation": "jsdoc2md \"src/navigation/**/*.jsx\" \"src/navigation/**/*.js\" > docs/navigation.md",
    "docs:types": "jsdoc2md \"src/types/**/*.js\" > docs/types.md",
    "docs:build": "npm run docs:clean && mkdirp docs && npm run docs:components && npm run docs:navigation && npm run docs:types",
    "docs:check": "npm run docs:build && git diff --exit-code docs",
    "docs:verify": "npm run lint && npm run docs:build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@rsuite/icons": "^1.4.0",
    "html-to-image": "^1.11.13",
    "import": "^0.0.6",
    "mermaid": "^11.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.9.6",
    "rsuite": "^5.83.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@swc/core": "1.9.3",
    "@swc/jest": "0.2.37",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^5.1.0",
    "cross-env": "7.0.3",
    "eslint": "^9.39.2",
    "eslint-formatter-html": "^2.7.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jscodeshift": "^17.3.0",
    "jsdoc": "^4.0.5",
    "jsdoc-to-markdown": "^9.1.3",
    "jsdom": "^27.4.0",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.1.2",
    "vite": "^7.2.2",
    "vitest": "^4.0.16"
  }
}
