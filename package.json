{
  "name": "react-portfolio",
  "private": true,
  "version": "1.1.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "codemod:fix-jsdoc": "jscodeshift -t scripts/codemods/fix-jsdoc-import-types.js src",
    "ci:gate": "npm-run-all --sequential lint:cleanup test diagrams:check diagrams:test && npm run docs:build",
    "dev": "vite",
    "diagrams:format": "node scripts/format-diagrams.js",
    "diagrams:lint": "node scripts/lint-diagrams.js",
    "diagrams:check": "npm-run-all diagrams:format && npm run diagrams:lint",
    "diagrams:assets": "node scripts/build-diagram-assets.js",
    "diagrams:test": "playwright test",
    "docs:clean": "rimraf docs",
    "docs:components": "jsdoc2md \"src/components/**/*.jsx\" \"src/components/**/*.js\" > docs/components.md",
    "docs:navigation": "jsdoc2md \"src/navigation/**/*.jsx\" \"src/navigation/**/*.js\" > docs/navigation.md",
    "docs:types": "jsdoc2md \"src/types/**/*.js\" > docs/types.md",
    "docs:build": "npm-run-all --sequential docs:clean diagrams:lint diagrams:assets docs:mkdir docs:components docs:navigation docs:types",
    "docs:diff": "npm run docs:build && git diff --exit-code docs",
    "docs:mkdir": "mkdirp docs",
    "docs:verify": "npm run lint:ci && npm run docs:build",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "lint:fix-safe": "eslint src --fix",
    "lint:cleanup": "npm run lint:keys && npm run lint:map-index && npm run lint:fix-safe && npm run lint",
    "lint:keys": "jscodeshift -t scripts/codemods/add-react-keys.js src",
    "lint:map-index": "jscodeshift -t scripts/codemods/fix-unused-map-index.js src",
    "lint:report": "eslint src --format html --output-file docs/lint-report.html",
    "start": "cross-env PORT=3000 vite",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "npm run prettier:fix",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run",
    "quality": "npm-run-all --sequential lint:cleanup docs:build test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-brands-svg-icons": "7.1.0",
    "@fortawesome/free-regular-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/react-fontawesome": "3.1.1",
    "@rsuite/icons": "1.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "html-to-image": "1.11.13",
    "mermaid": "11.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "2.0.5",
    "react-router": "6.30.3",
    "react-router-dom": "6.30.3",
    "rsuite": "5.83.3",
    "svg-pan-zoom": "3.6.2"
  },
  "devDependencies": {
    "@eslint/js": "8.57.1",
    "@mermaid-js/mermaid-cli": "11.12.0",
    "@mermaid-js/parser": "0.6.3",
    "@playwright/test": "1.57.0",
    "@swc/core": "1.9.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/coverage-v8": "4.0.16",
    "cross-env": "7.0.3",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-html": "2.7.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "61.5.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "eslint-plugin-unused-imports": "4.3.0",
    "eslint-plugin-vitest": "0.5.4",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "identity-obj-proxy": "3.0.0",
    "jscodeshift": "17.3.0",
    "jsdoc": "4.0.5",
    "jsdoc-to-markdown": "9.1.3",
    "jsdom": "27.4.0",
    "lint-staged": "16.2.7",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "3.7.4",
    "rimraf": "6.1.2",
    "vite": "5.4.21",
    "vitest": "4.0.16"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
